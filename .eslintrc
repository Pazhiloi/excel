{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "linebreak-style": "off",
    "indent": "off",
    "no-trailing-spaces": "off",
    "comma-spacing": "off",
    "object-curly-spacing": "off",
    "eol-last": "off",
    "require-jsdoc": "off",
    "space-before-blocks": "off",
    "no-unused-vars": "off",
    "no-multi-spaces": "off",
    "operator-linebreak": "off",
    "guard-for-in": "off"
  },

  "extends": ["eslint:recommended","google"]
}